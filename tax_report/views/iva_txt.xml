<odoo>
  <data>

      <record id="view_invoice_tree_iva_txt" model="ir.ui.view">
            <field name="name">account.invoice.tree.iva.txt</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_invoice_tree"/>
            <field name="mode">primary</field>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="attributes">
                    <attribute name="banner_route">/account/account_invoice_onboarding</attribute>
                </xpath>
                <field name="currency_id" position="attributes">
                    <attribute name="string">Invoice Currency</attribute>
                </field>
            </field>
      </record>

