<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="custom_view_account_journal_tree" model="ir.ui.view">
            <field name="name">custom.account.journal.tree</field>
            <field name="model">account.journal</field>
            <field name="priority">1</field>
            <field name="inherit_id" ref="account.view_account_journal_tree"/>
            <field name="arch" type="xml">

                <field name="code" position="after">
                    <field name="tax_code"/>
                </field>


            </field>
        </record>

        <record id="custom_view_account_journal_form" model="ir.ui.view">
            <field name="name">custom.account.journal.form</field>
            <field name="model">account.journal</field>
            <field name="priority">1</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                <field name="code" position="after">
                    <field name="tax_code"/>
                </field>
            </field>
        </record>

    </data>
</odoo>
