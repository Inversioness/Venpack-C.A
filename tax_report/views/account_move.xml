<odoo>
  <data>
     <record id="custom_view_account_move_form" model="ir.ui.view">
            <field name="name">custom.view.account.move.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">

                <field name="partner_id" position="after">
                    <field name="third_party_account_payment" invisible="1" />
                    <field name="fiscal_provider"  widget="res_partner_many2one" attrs="{'invisible': [('third_party_account_payment', '=', False)]}"/>
                </field>

                <field name="invoice_date" position="after">
                    <field name="x_tasa"/>
                </field>

                <page name="other_info" position="after">
                    <page string="Ordenes de Pago Info">
                        <field name="x_payment_ids"/>
                    </page>
                </page>

            </field>
        </record>
  </data>
</odoo>