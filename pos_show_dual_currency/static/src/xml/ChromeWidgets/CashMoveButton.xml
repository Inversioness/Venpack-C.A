<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="CashMoveButtonRefCurrency" owl="1">
        <div class="cash-move-button" t-on-click="onClickUSD">
            <i class="fa fa-money" aria-hidden="true"></i>
            <span style="padding-left: 10px;">Efectivo $ IN/OUT</span>
        </div>
    </t>

    <t t-name="CashInOutRefCurrencyChrome" t-inherit="point_of_sale.Chrome" t-inherit-mode="extension">
        <CashMoveButton position="after">
            <CashMoveButtonRefCurrency t-if="showCashMoveButton()"/>
        </CashMoveButton>
    </t>
</templates>
